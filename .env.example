# File: .env.example
# When adding additional environment variables, the schema in "/src/env.js"
# should be updated accordingly.
# Next Auth
# You can generate a new secret on the command line with:
# npx auth secret
# https://next-auth.js.org/configuration/options#secret

PORT=3222

NEXTAUTH_URL="https://"
NEXT_PUBLIC_NEXTAUTH_URL=https://

AUTH_SECRET= # Generated by create-t3-app.
AUTH_TRUST_HOST=

# Next Auth Discord Provider
AUTH_DISCORD_ID=
AUTH_DISCORD_SECRET=

# Drizzle
DB_ADMIN_USER=
DB_ADMIN_PASSWORD=
DB_HOST=
DB_PORT=
DB_NAME=
DATABASE_URL="postgres://user:pass@localhost:5432/my_db" # &sslmode=require&sslrootcert=certs/ca.pem

NEXT_PUBLIC_API_URL="https://" # trailing slash is required
STREAMING_KEY=

SONGBIRD_PUBLIC_API_URL="https://" # trailing slash is required
SONGBIRD_API_KEY=

ELECTRON_DEV_TOOLS=false