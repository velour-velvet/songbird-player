{
  "name": "starchild-music-frontend",
  "version": "0.3.0",
  "private": true,
  "type": "module",
  "email": "cirrus.okimasu@gmail.com",
  "author": "Cirrus Okimasu",
  "license": "MIT",
  "homepage": "https://starchild.music",
  "repository": {
    "type": "git",
    "url": "https://github.com/soulwax/starchild-music-frontend"
  },
  "bugs": {
    "url": "https://github.com/soulwax/starchild-music-frontend/issues"
  },
  "keywords": [
    "starchild",
    "music",
    "frontend",
    "nextjs",
    "tailwindcss",
    "typescript"
  ],
  "contributors": [
    {
      "name": "Cirrus Okimasu",
      "email": "cirrus.okimasu@gmail.com",
      "url": "https://github.com/soulwax"
    }
  ],
  "scripts": {
    "build": "next build",
    "check": "eslint . && tsc --noEmit",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "dev": "next dev --turbo --port 3222",
    "pm2:start": "npm run build && pm2 start ecosystem.config.cjs --env production",
    "pm2:dev": "pm2 start ecosystem.config.cjs --only starchild-music-frontend-dev",
    "pm2:reload": "pm2 reload ecosystem.config.cjs --env production",
    "pm2:restart": "pm2 restart ecosystem.config.cjs --update-env",
    "pm2:stop": "pm2 stop ecosystem.config.cjs",
    "pm2:delete": "pm2 delete ecosystem.config.cjs",
    "pm2:logs": "pm2 logs starchild-music-frontend-prod",
    "pm2:logs:dev": "pm2 logs starchild-music-frontend-dev",
    "pm2:logs:error": "pm2 logs starchild-music-frontend-prod --err",
    "pm2:status": "pm2 status",
    "pm2:monit": "pm2 monit",
    "pm2:flush": "pm2 flush",
    "pub": "npm run build && pm2 restart ecosystem.config.cjs --update-env",
    "deploy": "npm run build && pm2 reload ecosystem.config.cjs --env production --update-env",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "preview": "next build && next start",
    "start": "next start --port 3222",
    "typecheck": "tsc --noEmit",
    "electron:dev": "concurrently \"npm run dev\" \"wait-on http://localhost:3222 && electron .\"",
    "electron:prod": "node scripts/load-env-build.js \"cross-env ELECTRON_BUILD=true ELECTRON_PROD=true next build && npm run electron:prepare && electron .\"",
    "electron:prod:win": "npm run electron:build:win && .\\dist\\Starchild Music Setup 0.3.0.exe",
    "electron:prepare": "node scripts/load-env-build.js \"node electron/prepare-package.js\"",
    "electron:clean": "node -e \"require('fs').rmSync('dist', {recursive: true, force: true})\"",
    "electron:build": "npm run electron:clean && node scripts/load-env-build.js \"cross-env ELECTRON_BUILD=true next build\" && npm run electron:prepare && electron-builder",
    "electron:build:win": "npm run electron:clean && node scripts/load-env-build.js \"cross-env ELECTRON_BUILD=true next build\" && npm run electron:prepare && electron-builder --win",
    "electron:build:mac": "npm run electron:clean && node scripts/load-env-build.js \"cross-env ELECTRON_BUILD=true next build\" && npm run electron:prepare && electron-builder --mac",
    "electron:build:linux": "npm run electron:clean && node scripts/load-env-build.js \"cross-env ELECTRON_BUILD=true next build\" && npm run electron:prepare && electron-builder --linux"
  },
  "dependencies": {
    "@auth/drizzle-adapter": "^1.11.1",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@neondatabase/serverless": "^1.0.2",
    "@t3-oss/env-nextjs": "^0.12.0",
    "@tanstack/react-query": "^5.90.5",
    "@trpc/client": "^11.0.0",
    "@trpc/react-query": "^11.7.0",
    "@trpc/server": "^11.0.0",
    "drizzle-orm": "^0.41.0",
    "framer-motion": "^12.23.24",
    "lucide-react": "^0.548.0",
    "next": "^15.5.6",
    "next-auth": "^5.0.0-beta.30",
    "pg": "^8.16.3",
    "postgres": "^3.4.7",
    "react": "^19.2.0",
    "react-audio-visualize": "^1.2.0",
    "react-dom": "^19.2.0",
    "server-only": "^0.0.1",
    "superjson": "^2.2.3",
    "tone": "^15.1.22",
    "zod": "^3.25.76"
  },
  "main": "electron/main.cjs",
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@next/bundle-analyzer": "^16.0.1",
    "@svgr/cli": "^8.1.0",
    "@tailwindcss/postcss": "^4.1.16",
    "@types/node": "^20.19.25",
    "@types/pg": "^8.15.5",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "dotenv": "^17.2.3",
    "drizzle-kit": "^0.31.7",
    "electron": "^39.2.1",
    "electron-builder": "^25.1.8",
    "eslint": "^9.38.0",
    "eslint-config-next": "^15.5.6",
    "eslint-plugin-drizzle": "^0.2.3",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "svgo": "^4.0.0",
    "tailwindcss": "^4.1.16",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2",
    "wait-on": "^8.0.1",
    "webpack": "^5.102.1"
  },
  "build": {
    "appId": "com.starchild.music",
    "productName": "Starchild Music",
    "directories": {
      "output": "dist"
    },
    "files": [
      "electron/**/*",
      "package.json"
    ],
    "extraResources": [
      {
        "from": ".next/standalone",
        "to": ".next/standalone",
        "filter": [
          "**/*"
        ]
      }
    ],
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "public/icon.ico",
      "certificateSubjectName": "localhost",
      "signingHashAlgorithms": [
        "sha256"
      ],
      "signDlls": true,
      "sign": "./electron/sign.js"
    },
    "mac": {
      "target": [
        "dmg"
      ],
      "icon": "public/icon.icns",
      "category": "public.app-category.music"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "icon": "public/icon.png",
      "category": "Audio"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  },
  "ct3aMetadata": {
    "initVersion": "7.39.3"
  }
}