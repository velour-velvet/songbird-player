{
  "name": "darkfloor-art",
  "version": "0.6.7",
  "description": "A modern music streaming and discovery platform with intelligent recommendations, advanced audio features, and a beautiful user interface",
  "private": true,
  "type": "module",
  "email": "cirrus.okimasu@gmail.com",
  "author": "Cirrus Okimasu",
  "license": "MIT",
  "homepage": "https://darkfloor.art",
  "repository": {
    "type": "git",
    "url": "https://github.com/soulwax/starchild-music-frontend"
  },
  "bugs": {
    "url": "https://github.com/soulwax/starchild-music-frontend/issues"
  },
  "keywords": [
    "darkfloor",
    "music",
    "frontend",
    "nextjs",
    "tailwindcss",
    "typescript",
    "electron",
    "drizzle-orm",
    "trpc",
    "postgres"
  ],
  "contributors": [
    {
      "name": "Christian / soulwax@github",
      "email": "soulwax@nandcore.com",
      "url": "https://github.com/soulwax"
    }
  ],
  "scripts": {
    "clean": "rm -rf .next node_modules .turbo dist",
    "clean:win": "rmdir /s /q .next && rmdir /s /q node_modules && rmdir /s /q .turbo && rmdir /s /q dist",
    "build:analyzer": "cross-env ANALYZE=true next build",
    "build": "next build",
    "check": "eslint . && tsc --noEmit",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "dev": "cross-env NODE_ENV=development node scripts/server.js",
    "dev:next": "next dev --turbo --port 3222",
    "dev:win": "cross-env NODE_ENV=development node scripts/server.js",
    "pm2:start": "pm2 start ecosystem.config.cjs --only darkfloor-art-prod --env production",
    "pm2:start:build": "npm run build && npm run pm2:start",
    "pm2:dev": "pm2 start ecosystem.config.cjs --only darkfloor-art-dev --env development",
    "pm2:reload": "pm2 reload darkfloor-art-prod --update-env",
    "pm2:restart": "pm2 restart darkfloor-art-prod --update-env",
    "pm2:stop": "pm2 stop ecosystem.config.cjs",
    "pm2:delete": "pm2 delete ecosystem.config.cjs",
    "pm2:logs": "pm2 logs darkfloor-art-prod",
    "pm2:logs:dev": "pm2 logs darkfloor-art-dev",
    "pm2:logs:error": "pm2 logs darkfloor-art-prod --err",
    "pm2:status": "pm2 status",
    "pm2:monit": "pm2 monit",
    "pm2:flush": "pm2 flush",
    "pub": "npm run build && pm2 restart darkfloor-art-prod --update-env",
    "deploy": "npm run build && pm2 reload darkfloor-art-prod --update-env",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "preview": "next build && next start",
    "start": "cross-env NODE_ENV=production node scripts/server.js",
    "start:next": "next start --port 3222",
    "start:win": "cross-env NODE_ENV=production node scripts/server.js",
    "typecheck": "tsc --noEmit",
    "electron:dev": "concurrently \"npm run dev\" \"wait-on http://localhost:3222 && electron .\"",
    "electron:dev:win": "concurrently \"npm run dev:win\" \"wait-on http://localhost:3222 && electron .\"",
    "electron:prod": "node scripts/load-env-build.js \"cross-env ELECTRON_BUILD=true ELECTRON_PROD=true next build && npm run electron:prepare && electron .\"",
    "electron:prod:win": "npm run electron:build:win && \".\\dist\\darkfloor Setup.exe\"",
    "electron:build:dev:win": "npm run electron:clean:win && node scripts/load-env-build.js \"cross-env ELECTRON_BUILD=true ELECTRON_DEV_TOOLS=true next build\" && npm run electron:prepare && electron-builder --win --dir",
    "electron:prepare": "node scripts/load-env-build.js \"node electron/prepare-package.js\"",
    "electron:clean": "node -e \"require('fs').rmSync('dist', {recursive: true, force: true})\"",
    "electron:clean:win": "if exist dist rmdir /s /q dist",
    "electron:build": "npm run electron:clean && node scripts/load-env-build.js \"cross-env ELECTRON_BUILD=true next build\" && npm run electron:prepare && electron-builder",
    "electron:build:win": "npm run electron:clean && node scripts/load-env-build.js \"cross-env ELECTRON_BUILD=true next build\" && npm run electron:prepare && electron-builder --win",
    "electron:build:mac": "npm run electron:clean && node scripts/load-env-build.js \"cross-env ELECTRON_BUILD=true next build\" && npm run electron:prepare && electron-builder --mac",
    "electron:build:linux": "npm run electron:clean && node scripts/load-env-build.js \"cross-env ELECTRON_BUILD=true next build\" && npm run electron:prepare && electron-builder --linux"
  },
  "dependencies": {
    "@auth/drizzle-adapter": "^1.11.1",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@neondatabase/serverless": "^1.0.2",
    "@t3-oss/env-nextjs": "^0.12.0",
    "@tanstack/react-query": "^5.90.12",
    "@tanstack/react-virtual": "^3.13.13",
    "@trpc/client": "^11.0.0",
    "@trpc/react-query": "^11.8.1",
    "@trpc/server": "^11.0.0",
    "@use-gesture/react": "^10.3.1",
    "chalk": "^5.6.2",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.41.0",
    "framer-motion": "^12.23.26",
    "lucide-react": "^0.548.0",
    "morgan": "^1.10.1",
    "next": "^15.5.9",
    "next-auth": "^5.0.0-beta.30",
    "next-pwa": "^5.6.0",
    "pg": "^8.16.3",
    "postgres": "^3.4.7",
    "react": "^19.2.3",
    "react-audio-visualize": "^1.2.0",
    "react-dom": "^19.2.3",
    "server-only": "^0.0.1",
    "superjson": "^2.2.6",
    "tone": "^15.1.22",
    "vaul": "^1.1.2",
    "zod": "^3.25.76"
  },
  "main": "electron/main.cjs",
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@next/bundle-analyzer": "^16.1.1",
    "@svgr/cli": "^8.1.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/morgan": "^1.9.10",
    "@types/node": "^20.19.27",
    "@types/pg": "^8.16.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "autoprefixer": "^10.4.23",
    "concurrently": "^9.2.1",
    "cross-env": "^7.0.3",
    "drizzle-kit": "^0.31.8",
    "electron": "^39.2.7",
    "electron-builder": "^25.1.8",
    "eslint": "^9.39.2",
    "eslint-config-next": "^15.5.9",
    "eslint-plugin-drizzle": "^0.2.3",
    "postcss": "^8.5.6",
    "prettier": "^3.7.4",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "svgo": "^4.0.0",
    "tailwindcss": "^4.1.16",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.1",
    "wait-on": "^8.0.5",
    "webpack": "^5.104.1"
  },
  "build": {
    "appId": "com.darkfloor.art",
    "productName": "darkfloor",
    "directories": {
      "output": "dist"
    },
    "files": [
      "electron/**/*",
      "package.json"
    ],
    "extraResources": [
      {
        "from": ".next/standalone",
        "to": ".next/standalone",
        "filter": [
          "**/*"
        ]
      }
    ],
    "win": {
      "target": [
        "nsis"
      ],
      "executableName": "darkfloor",
      "icon": "public/icon.ico",
      "sign": false
    },
    "mac": {
      "target": [
        "dmg"
      ],
      "icon": "public/icon.icns",
      "category": "public.app-category.music"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "icon": "public/icon.png",
      "category": "Audio"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "artifactName": "${productName} Setup.${ext}"
    }
  },
  "ct3aMetadata": {
    "initVersion": "7.39.3"
  },
  "packageManager": "npm@11.6.2"
}